{
    "name": "iclrs/nova-sortable",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "rutorika/sortable": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Iclrs\\NovaSortable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iclrs\\NovaSortable\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
